<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
	"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>GWTOAuthLoginDemo.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>OAuthLoginService</servlet-name>
		<servlet-class>com.example.GWTOAuthLoginDemo.server.rpc.OAuthLoginServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>OAuthLoginService</servlet-name>
		<url-pattern>/com.example.GWTOAuthLoginDemo.GWTOAuthLoginDemo/OAuthLoginService</url-pattern>
	</servlet-mapping>

</web-app>
